package com.xworkz.MethodOverloading;

import MethodOverloading.MethodOverloading;

public class Runner {

	public static void main(String[] args) {
			 
		MethodOverloading methodover=new MethodOverloading();
		
		methodover.display();
		methodover.display(85.92);
		methodover.display(50, "Pitti");
		methodover.display(20, "Gicchi");
	}

}
